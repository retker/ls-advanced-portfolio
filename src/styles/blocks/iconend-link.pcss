.iconend-link {
    color: $links-color;
    font-size: 18px;
    font-weight: bold;
    display: inline-flex;
    align-items: center;

    &::before {
        content: "";
        display: block;
        width: 15px;
        height: 15px;
        background: svg-load('link.svg', fill=$links-color) center center no-repeat;
        margin-right: 20px;
        align-items: center;
    }
}
